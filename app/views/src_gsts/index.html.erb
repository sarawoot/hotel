<div class="page-header">
  <h3><%= raw("#{t "activemodel.models.src_gst"}") %></h3>
</div>
<div class="well" ><%= link_to t("html.add"), new_src_gst_path, class: "btn btn-primary"  %></div>
<center><%= paginate @src_gsts %></center>
<table class="table table-hover table-bordered table-striped">
  <thead>
    <tr>
      <th><%= raw("#{t "activemodel.attributes.src_gst.name"}") %></th>
      <th><%= raw("#{t "activemodel.attributes.src_gst..used"}") %></th>
      <th><%= "#{t("html.menu")}"%></th>
    </tr>
  </thead>
<% @src_gsts.each do |src_gst| %>
  <tr>
    <td><%= src_gst.name %></td>
    <td><%= used(src_gst.used) %></td>
    <td><%= link_to raw("<i class='icon-edit'></i>#{ t "html.edit"}"), edit_src_gst_path(src_gst), class: "btn" %></td>
  </tr>
<% end %>
</table>