<div class="page-header">
  <h3><%= raw("#{t "activemodel.models.agent"}") %></h3>
</div>
<div class="well" ><%= link_to t("html.add"), new_agent_path, class: "btn btn-primary"  %></div>
<center><%= paginate @agents %></center>
<table class="table table-hover table-bordered table-striped">
  <thead>
    <tr>
      <th><%= raw("#{t "activemodel.attributes.agent.name"}") %></th>
      <th><%= raw("#{t "activemodel.attributes.agent..used"}") %></th>
      <th><%= "#{t("html.menu")}"%></th>
    </tr>
  </thead>
<% @agents.each do |agent| %>
  <tr>
    <td><%= agent.name %></td>
    <td><%= used(agent.used) %></td>
    <td><%= link_to raw("<i class='icon-edit'></i>#{ t "html.edit"}"), edit_agent_path(agent), class: "btn" %></td>
  </tr>
<% end %>
</table>