<div class="page-header">
  <h3><%= raw("#{t "activemodel.models.gst_level"}") %></h3>
</div>
<div class="well" ><%= link_to t("html.add"), new_gst_level_path, class: "btn btn-primary"  %></div>
<center><%= paginate @gst_levels %></center>
<table class="table table-hover table-bordered table-striped">
  <thead>
    <tr>
      <th><%= raw("#{t "activemodel.attributes.gst_level.name"}") %></th>
      <th><%= raw("#{t "activemodel.attributes.gst_level..used"}") %></th>
      <th><%= "#{t("html.menu")}"%></th>
    </tr>
  </thead>
<% @gst_levels.each do |gst_level| %>
  <tr>
    <td><%= gst_level.name %></td>
    <td><%= used(gst_level.used) %></td>
    <td><%= link_to raw("<i class='icon-edit'></i>#{ t "html.edit"}"), edit_gst_level_path(gst_level), class: "btn" %></td>
  </tr>
<% end %>
</table>
