<table class="table table-hover table-bordered" id="stay_list_tb<%= @gen_t %>_gen_t">
  <thead>
    <th><%= raw(t("html.check_in"))%></th>
    <th><%= raw(t("activemodel.attributes.stay_list.fname"))%></th>
    <th><%= raw(t("activemodel.attributes.stay_list.lname"))%></th>
    <th><%= raw(t("activemodel.attributes.stay_list.abf_rate"))%></th>
    <th><%= raw(t("activemodel.attributes.stay_list.ext_bed_rate"))%></th>
    <th><%= raw(t("html.menu"))%></th>
  </thead>
  </thead>
  <tbody>
  <%= f.simple_fields_for  :stay_lists do |b| %>
    <tr class="fields">
      <td><%= b.check_box :status, {},'1','2' %></td><!--1=check in,2=cancel-->
      <td><%= b.text_field :fname, style: "width:120px;" %></td>
      <td><%= b.text_field :lname, style: "width:120px;" %></td>
      <td><%= b.number_field :abf_rate, style: "width:120px;" %></td>
      <td><%= b.number_field :ext_bed_rate, style: "width:120px;" %></td>
      <td><%= link_to_remove_fields('<i class="icon-remove"></i>'.html_safe, b) %></td>
    </tr>
  <% end %>
  </tbody>  
</table>
<%= content_for :js_footer do%>
<script type="text/javascript">
    $("input[date_time=1]").datetimepicker({format: "dd/mm/yyyy hh:ii"});
</script>
<% end %>
