<div class="page-header">
  <h3><%= raw("#{t "activemodel.models.gst_type"}") %></h3>
</div>
<div class="well" ><%= link_to t("html.add"), new_gst_type_path, class: "btn btn-primary"  %></div>
<center><%= paginate @gst_types %></center>
<table class="table table-hover table-bordered table-striped">
  <thead>
    <tr>
      <th><%= raw("#{t "activemodel.attributes.gst_type.name"}") %></th>
      <th><%= raw("#{t "activemodel.attributes.gst_type..used"}") %></th>
      <th><%= "#{t("html.menu")}"%></th>
    </tr>
  </thead>
<% @gst_types.each do |gst_type| %>
  <tr>
    <td><%= gst_type.name %></td>
    <td><%= used(gst_type.used) %></td>
    <td><%= link_to raw("<i class='icon-edit'></i>#{ t "html.edit"}"), edit_gst_type_path(gst_type), class: "btn" %></td>
  </tr>
<% end %>
</table>